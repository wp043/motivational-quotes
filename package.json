{
  "name": "motivational-quote",
  "displayName": "motivational-quote",
  "description": "displays a motivational (maybe) quote in the status bar",
  "version": "0.0.1",
  "publisher": "YingtongPan",
  "repository": {
        "type": "git",
        "url": "https://github.com/wp043/motivational-quotes.git"
    },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:motivationalQuotes.shuffleQuote"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
        {
            "command": "motivationalQuotes.shuffleQuote",
            "title": "Shuffle Motivational Quote"
        }
    ],
    "configuration": {
        "type": "object",
        "properties": {
            "motivationalQuotes.userQuotes": {
                "type": "array",
                "description": "User-defined motivational quotes.",
                "items": {
                    "type": "string"
                },
                "default": []
            }
        }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "vscode": "^1.1.37",
    "vscode-test": "^1.6.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  }
}
